{
  "name": "@vfat-io/sickle-wrapper-sdk",
  "version": "0.0.1",
  "description": "TypeScript SDK for interacting with SickleWrapper contracts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "author": "vfat.io",
  "license": "ISC",
  "type": "commonjs",
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "typescript": "^5.9.2",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@vfat-io/sickle-sdk": "file:../../sickle-sdk",
    "viem": "^2.33.3"
  }
}
